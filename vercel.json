{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb",
        "memory": 1024,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "MYSQL2_FORCE_PURE_JS": "1"
  },
  "installCommand": "npm install --ignore-scripts=false"
}